{
  "[javascript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  },
  "[typescript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
    "editor.rulers": [
      80
    ]
  },
  "[vue]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.fixAll.eslint": "always"
  },
}