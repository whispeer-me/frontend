{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Nuxt: Server-side",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "timeout": 30000,
      "console": "integratedTerminal"
    },
    {
      "name": "Vitest Debug",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "test:debug"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}